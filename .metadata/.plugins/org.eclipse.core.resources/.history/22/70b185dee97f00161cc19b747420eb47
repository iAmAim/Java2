package com.basic;

public class Numbers {

	private int delay = 0;

	public Numbers(int delay) {
		this.delay = delay;
	}

	public int getDelay() {
		return delay;
	}

	public void setDelay(int delay) {
		this.delay = delay;
	}

	public synchronized void printNumbers() throws InterruptedException {

		for (int i = 0; i < 10; i++) {
			System.out.println("number " + i);
			wait(this.delay);
		}
	}

}
