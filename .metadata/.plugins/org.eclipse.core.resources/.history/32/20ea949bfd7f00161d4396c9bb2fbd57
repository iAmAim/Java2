package com.producerconsumer;

import java.util.List;

public class Consumer implements Runnable{

	private Buffer buffer;
	private String decorator;
	private List<Integer> taskQueue;
	public Consumer(List<Integer> taskQueue, Buffer buffer, String decorator) {
		this.buffer = buffer;
		this.decorator = decorator;
		this.taskQueue = taskQueue;
	}

	@Override
	public void run() {
		
		String data = null;
		while(!("end".equals(data))){
			data = buffer.getMessage();
			System.out.println("Read: " + decorator + data);
		}
		System.out.println("Consumer ends.");
	}

}
