package designpatterns;

public class Driver {

	public static void main(String[] args) {

		SimpleRobot robot = new SimpleRobot("clockwerk");
		SimpleRobot techies = new SimpleRobot("techies");
		RobotObserver observerWard = new RobotObserver();
		robot.register(observerWard);
		techies.register(observerWard);
		robot.move(12, 24);
		robot.move(12, 26);
	}

}
